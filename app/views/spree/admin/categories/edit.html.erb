

<% content_for :page_title do %>
  <%= link_to Spree.t(:category), spree.admin_categories_url %> /
  <%= Spree.t(:edit_category) %>
<% end %>

<%#= render partial: 'spree/admin/shared/combo_tabs', locals: {current: :details} %>
<%= render :partial => 'spree/admin/shared/error_messages', :locals => { :target => @category } %>


<%= nested_form_for [:admin, @category], :html => { :multipart => true } do |f| %>
  <fieldset data-hook="new_category">
    <%= render partial: 'form', locals: { f: f } %>
    <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>


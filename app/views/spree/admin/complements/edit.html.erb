<% content_for :page_actions do %>
  <% if can?(:create, Complement) %>
    <%= button_link_to Spree.t(:new_complement), new_object_url, { class: "btn-success", icon: 'add', id: 'admin_new_complement' } %>
  <% end %>
<% end %>

<% content_for :page_title do %>
  Complemento: 
  <%= @complement.name %>
<% end %>

<%#= render partial: 'spree/admin/shared/complement_tabs', locals: {current: :details} %>
<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @complement } %>

<%= nested_form_for [:admin, @complement], method: :put, html: { multipart: true } do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>

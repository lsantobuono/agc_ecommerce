
<div data-hook="admin_complement_form_fields">

  <div class="row">
    <div class="col-md-8" data-hook="admin_complement_form_right">
      <div class="row">
        <div class="col-md-4">
          <div data-hook="admin_complement_form_name" class="omega two columns">
            <%= f.field_container :name, class: ['form-group'] do %>
              <%= f.label :name, Spree.t(:name) %>
              <%= f.text_field :name, class: 'form-control' %>
              <%= f.error_message_on :name %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div data-hook="admin_downloadable_file_form_fields" class="row">
  <div class="col-md-6">
    <div data-hook="admin_downloadable_file_form_name" class="omega two columns">
      <%= f.field_container :name, class: ['form-group'] do %>
        <%= f.label :name, Spree.t(:name) %>
        <%= f.text_field :name, class: 'form-control' %>
        <%= f.error_message_on :name %>
      <% end %>
    </div>
    <div data-hook="admin_downloadable_file_form_file" class="omega two columns">
      <%= f.field_container :file, class: ['form-group'] do %>
        <%= f.label :file, Spree.t(:file) %>
        <%= f.file_field :file, class: 'form-control' %>
        <%= f.error_message_on :file %>
      <% end %>
    </div>
    <div data-hook="admin_downloadable_file_form_private" class="omega two columns">
      <%= f.field_container :private, class: ['form-group'] do %>
        <%= f.label :private, Spree.t(:private) %>
        <%= f.check_box :private, class: 'form-control' %>
        <%= f.error_message_on :private %>
      <% end %>
    </div>
  </div>
</div>
